{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "start": {
      "dependsOn": [
        "^start"
      ]
    },
    "start:dev": {
      "dependsOn": [
        "^start:dev"
      ]
    },
    "start:prod": {
      "dependsOn": [
        "^start:prod"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "cache": true
    },
    "test:e2e": {
      "dependsOn": [
        "^test:e2e"
      ]
    },
    "prettier": {
      "dependsOn": [
        "^prettier"
      ],
      "cache": true
    }
  },
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "eslint:lint"
      }
    },
    {
      "plugin": "@nx/jest/plugin",
      "options": {
        "targetName": "test"
      }
    }
  ]
}
